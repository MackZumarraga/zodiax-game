generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int    @id @default(autoincrement())
  name      String
  email     String @unique
  currentHp Int
  maxHp     Int
  currentMp Int
  maxMp     Int
}
